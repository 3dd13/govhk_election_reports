{
  "definitions": {
    "AccumulatedVotesByHour": {
      "type": "object",
      "title": "AccumulatedVotesByHour",
      "required": [
        "countedAt",
        "numberOfVotes"
      ],
      "additionalProperties": false,
      "properties": {
        "countedAt": {
          "type": "string",
          "title": "HH:MM",
          "default": "00:00",
          "examples": [],
          "pattern": "^([0-2][0-9]:[0-5][0-9])$"
        },
        "numberOfVotes": {
          "type": "number",
          "title": "Number of Votes",
          "default": 0
        }
      }
    },
    "SummaryStatistics": {
      "type": "object",
      "required": [
        "availableVotes",
        "submittedVotes",
        "voidedVotes",
        "accumulatedVotesByHour",
        "candidateApplications",
        "receivedComplaints"
      ],
      "properties": {
        "availableVotes": {
          "type": "number",
          "title": "Available Votes",
          "default": 0
        },
        "submittedVotes": {
          "type": "number",
          "title": "Submitted Votes",
          "default": 0
        },
        "voidedVotes": {
          "type": "number",
          "title": "Voided Votes",
          "default": 0
        },
        "candidateApplications": {
          "type": "object",
          "title": "Candidate Applications",
          "required": [
            "receivedApplications",
            "approvedApplications"
          ],
          "properties": {
            "receivedApplications": {
              "type": "number",
              "title": "Received Applications",
              "default": 0
            },
            "approvedApplications": {
              "type": "number",
              "title": "Approved Applications",
              "default": 0
            }
          }
        },
        "accumulatedVotesByHour": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AccumulatedVotesByHour"
          }
        },
        "receivedComplaints": {
          "type": "object",
          "title": "Received Complaints",
          "required": [
            "againstEac",
            "againstRo",
            "againstPolice",
            "againstIcac",
            "againstPros"
          ],
          "additionalProperties": false,
          "properties": {
            "againstEac": {
              "type": "number",
              "title": "Number of Complaints against EAC",
              "default": 0
            },
            "againstRo": {
              "type": "number",
              "title": "Number of Complaints against RO",
              "default": 0
            },
            "againstPolice": {
              "type": "number",
              "title": "Number of Complaints against Police",
              "default": 0
            },
            "againstIcac": {
              "type": "number",
              "title": "Number of Complaints against ICAC",
              "default": 0
            },
            "againstPros": {
              "type": "number",
              "title": "Number of Complaints against PROs",
              "default": 0
            }
          }
        }
      }
    },
    "Candidate": {
      "type": "object",
      "title": "Candidate",
      "required": [
        "candidateNumber",
        "nameZh",
        "nameEn",
        "receivedVotes",
        "elected",
        "gender",
        "occupationEn",
        "occupationZh",
        "policticalAffiliationEn",
        "policticalAffiliationZh"
      ],
      "additionalProperties": false,
      "properties": {
        "candidateNumber": {
          "type": "number",
          "title": "Candidate Number",
          "default": 0
        },
        "nameZh": {
          "type": "string",
          "title": "Name (Chinese)",
          "default": "",
          "examples": [
            "test"
          ],
          "pattern": "^(.*)$"
        },
        "nameEn": {
          "type": "string",
          "title": "Name (English)",
          "default": "",
          "examples": [
            "test"
          ],
          "pattern": "^(.*)$"
        },
        "receivedVotes": {
          "type": "number",
          "title": "Received Votes",
          "default": 0
        },
        "elected": {
          "type": "boolean",
          "title": "Elected?",
          "default": false
        },
        "gender": {
          "type": "string",
          "title": "Gender",
          "default": "",
          "pattern": "male|female"
        },
        "occupationEn": {
          "type": "string",
          "title": "Occupation (English)"
        },
        "occupationZh": {
          "type": "string",
          "title": "Occupation (Chinese)"
        },
        "policticalAffiliationEn": {
          "type": "string",
          "title": "Political Affiliation (English)"
        },
        "policticalAffiliationZh": {
          "type": "string",
          "title": "Political Affiliation (Chinese)"
        }
      }
    },
    "ConstituencyStatistics": {
      "type": "object",
      "required": [
        "nameZh",
        "nameEn",
        "constituencyCode",
        "constituencyType",
        "autoDulyElected",
        "availableVotes",
        "submittedVotes",
        "voidedVotes",
        "accumulatedVotesByHour",
        "candidates"
      ],
      "properties": {
        "nameZh": {
          "type": "string"
        },
        "nameEn": {
          "type": "string"
        },
        "districtNameZh": {
          "type": "string"
        },
        "districtNameEn": {
          "type": "string"
        },
        "constituencyCode": {
          "type": "string"
        },
        "constituencyType": {
          "type": "string",
          "pattern": "geographical|functional|district"
        },
        "autoDulyElected": {
          "type": "boolean",
          "default": false
        },
        "availableVotes": {
          "type": "number",
          "title": "Available Votes",
          "default": 0
        },
        "submittedVotes": {
          "type": "number",
          "title": "Submitted Votes",
          "default": 0
        },
        "voidedVotes": {
          "type": "number",
          "title": "Voided Votes",
          "default": 0
        },
        "accumulatedVotesByHour": {
          "type": "array",
          "title": "Accumulated Votes By Hour Array",
          "items": {
            "$ref": "#/definitions/AccumulatedVotesByHour"
          }
        },
        "candidates": {
          "type": "array",
          "title": "Candidate Array",
          "items": {
            "$ref": "#/definitions/Candidate"
          }
        }
      }
    },
    "DateTimeRange": {
      "type": "object",
      "title": "DateTimeRange",
      "required": [
        "startedAt",
        "endedAt"
      ],
      "additionalProperties": false,
      "properties": {
        "startedAt": {
          "type": "string",
          "title": "Start Time",
          "pattern": "^([0-9]{4}/[0-9]{2}/[0-9]{2} [0-2][0-9]:[0-5][0-9])$"
        },
        "endedAt": {
          "type": "string",
          "title": "End Time",
          "pattern": "^([0-9]{4}/[0-9]{2}/[0-9]{2} [0-2][0-9]:[0-5][0-9])$"
        }
      }
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "/ElectionResult",
  "type": "object",
  "title": "Election Result Schema",
  "required": [
    "nameZh",
    "nameEn",
    "code",
    "dataSource",
    "electionPeriod",
    "candidateApplicationPeriod",
    "complaintsHandlingPeriod",
    "overallStatistics",
    "constituencyStatistics"
  ],
  "additionalProperties": false,
  "properties": {
    "nameZh": {
      "type": "string",
      "title": "Name (Chinese)",
      "default": "",
      "examples": [
        "test"
      ],
      "pattern": "^(.*)$"
    },
    "nameEn": {
      "type": "string",
      "title": "Name (English)",
      "default": "",
      "examples": [
        "test"
      ],
      "pattern": "^(.*)$"
    },
    "code": {
      "type": "string",
      "title": "Unique Code",
      "default": "",
      "examples": [
        "test"
      ],
      "pattern": "^(.*)$"
    },
    "dataSource": {
      "type": "string",
      "title": "URL of the data source",
      "default": "",
      "examples": [
        "test"
      ],
      "pattern": "^(.*)$"
    },
    "electionPeriod": {
      "title": "Election Period",
      "$ref": "#/definitions/DateTimeRange"
    },
    "electionPeriodDpss": {
      "title": "Election Period (DPSS)",
      "$ref": "#/definitions/DateTimeRange"
    },
    "candidateApplicationPeriod": {
      "title": "Candidate Application Period (DPSS)",
      "$ref": "#/definitions/DateTimeRange"
    },
    "complaintsHandlingPeriod": {
      "title": "Complaints Handling Period (DPSS)",
      "$ref": "#/definitions/DateTimeRange"
    },
    "overallStatistics": {
      "$ref": "#/definitions/SummaryStatistics"
    },
    "constituencyStatistics": {
      "type": "array",
      "title": "Constituency Array",
      "items": {
        "$ref": "#/definitions/ConstituencyStatistics"
      }
    }
  }
}